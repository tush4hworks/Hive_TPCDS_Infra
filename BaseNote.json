{"paragraphs":[{"text":"%pyspark\nsc.version","user":"user1","dateUpdated":"2017-12-06T15:49:27+0900","config":{"editorMode":"ace/mode/python","colWidth":12,"results":{},"enabled":false,"editorSetting":{"editOnDblClick":false,"language":"python"}},"settings":{"params":{},"forms":{}},"apps":[],"jobName":"paragraph_1512471404802_-605852484","id":"20171111-030419_133138600","dateCreated":"2017-12-05T19:56:44+0900","dateStarted":"2017-12-06T15:49:13+0900","dateFinished":"2017-12-06T15:49:13+0900","status":"FINISHED","errorMessage":"","progressUpdateIntervalMs":500,"focus":true,"$$hashKey":"object:12649"},{"text":"%pyspark\n\nimport itertools\nfrom pyspark.sql.types import *\n\ntext=sc.textFile('/tmp/zepin.csv')\ndata=text.filter(lambda x:x.split(',')[3]!='NA').map(lambda x:x.split(','))\ndf=sqlContext.createDataFrame(data,['QUERY','SETTING','RUN','DURATION'])\ndf=df.withColumn('RUN',df['RUN'].cast('integer')).withColumn('DURATION',df['DURATION'].cast('double'))\n\n\nnot_runs=text.filter(lambda x:x.split(',')[3]=='NA').map(lambda x:x.split(','))\nif not(not_runs.count())==0:\n    nr_df=sqlContext.createDataFrame(not_runs,['QUERY','SETTING','RUN','RESULT'])\nelse:\n    nr_schema=StructType([StructField('QUERY', StringType(),True),StructField('SETTING', StringType(),True),StructField('RUN', IntegerType(),True),StructField('DURATION', DoubleType(), True)])\n    nr_df=sqlContext.createDataFrame(sc.emptyRDD(), nr_schema)\n\n\navg_times=df.groupBy(['QUERY','SETTING']).avg('DURATION').withColumnRenamed('avg(DURATION)','avgdur')\navg_times.cache()\n\nmin_times=df.groupBy('QUERY').min('DURATION').withColumnRenamed('min(DURATION)','mindur').withColumnRenamed('QUERY','quer')\nqueryMins=min_times.join(df,[min_times.quer==df.QUERY,min_times.mindur==df.DURATION]).select('QUERY','SETTING','DURATION').sort(\"QUERY\")\noptimalSetting=min_times.join(df,[min_times.quer==df.QUERY,min_times.mindur==df.DURATION]).select('QUERY','SETTING','DURATION').groupBy('SETTING').count().sort(\"count\", ascending=False)\n\ntop_10=sqlContext.createDataFrame(sc.parallelize([]),avg_times.schema)\nfor setting in [set.SETTING for set in avg_times.select('SETTING').distinct().collect()]:\n    top_10=top_10.unionAll(avg_times[avg_times.SETTING==setting].sort('avgdur',ascending=False).limit(10))","user":"user1","dateUpdated":"2017-12-06T15:49:39+0900","config":{"editorMode":"ace/mode/python","colWidth":12,"results":{},"enabled":false,"editorSetting":{"language":"python"}},"settings":{"params":{},"forms":{}},"apps":[],"jobName":"paragraph_1512471404803_-606237233","id":"20171123-183108_299201605","dateCreated":"2017-12-05T19:56:44+0900","dateStarted":"2017-12-06T15:49:13+0900","dateFinished":"2017-12-06T15:49:18+0900","status":"FINISHED","errorMessage":"","progressUpdateIntervalMs":500,"$$hashKey":"object:12650"},{"text":"%pyspark\n\n#General Statistics.\n#Settings to be entered as a comma-separated list\n\nsetsInfo=z.input('settings').split(',')\n\nsqlContext.registerDataFrameAsTable(df[df.SETTING.isin(setsInfo)],'mainTable')\nsqlContext.registerDataFrameAsTable(nr_df[nr_df.SETTING.isin(setsInfo)],'failedRuns') \nsqlContext.registerDataFrameAsTable(avg_times[avg_times.SETTING.isin(setsInfo)],'AVG_DURATION') \nsqlContext.registerDataFrameAsTable(queryMins,'queryMins')\nsqlContext.registerDataFrameAsTable(optimalSetting[optimalSetting.SETTING.isin(setsInfo)],'optimalSetting') \nsqlContext.registerDataFrameAsTable(top_10[top_10.SETTING.isin(setsInfo)],'TOP_10')\n","user":"user1","dateUpdated":"2017-12-06T15:49:47+0900","config":{"editorMode":"ace/mode/python","colWidth":12,"results":{},"enabled":false,"editorSetting":{"language":"python"}},"settings":{"params":{"settings":"setdefault,setdefault1,setting1,setting2,setting3"},"forms":{"settings":{"name":"settings","displayName":"settings","type":"input","defaultValue":"","hidden":false,"$$hashKey":"object:13102"}}},"apps":[],"jobName":"paragraph_1512471404803_-606237233","id":"20171123-183111_1526769018","dateCreated":"2017-12-05T19:56:44+0900","dateStarted":"2017-12-06T15:49:14+0900","dateFinished":"2017-12-06T15:49:18+0900","status":"FINISHED","errorMessage":"","progressUpdateIntervalMs":500,"$$hashKey":"object:12651"},{"text":"%pyspark\nfrom pyspark.sql.functions import abs\nfrom pyspark.sql.functions import least\ncomp=z.input('settings').split(',')\n\n\nset1df=avg_times[avg_times['SETTING']==comp[0]]\nset2df=avg_times[avg_times['SETTING']==comp[1]].withColumnRenamed('QUERY','QUERY2').withColumnRenamed('SETTING','SETTING2').withColumnRenamed('avgdur','avgdur2')\n\njoined=set1df.join(set2df,set1df.QUERY==set2df.QUERY2,'outer')\ncnt1=joined.where('avgdur<avgdur2 or (avgdur is not NULL and avgdur2 is NULL)').count()\ncnt2=joined.where('avgdur2<avgdur or (avgdur2 is not NULL and avgdur is NULL)').count()\ncompRow=[[comp[0],comp[1],cnt1,cnt2]]\n\nsqlContext.registerDataFrameAsTable(sqlContext.createDataFrame(sc.parallelize(compRow),['COMPARE1','COMPARE2','COUNT1','COUNT2']),'OneOnOne')\n\nvariate=joined.withColumn('variate',abs((joined.avgdur-joined.avgdur2)/least('avgdur','avgdur2'))).orderBy('variate',ascending=False).drop('variate').limit(10)\n\nsqlContext.registerDataFrameAsTable(variate,'varTable')\n","user":"user1","dateUpdated":"2017-12-06T15:18:03+0900","config":{"editorMode":"ace/mode/python","colWidth":12,"results":{},"enabled":false,"editorSetting":{"language":"python"}},"settings":{"params":{"settings":""},"forms":{"settings":{"name":"settings","displayName":"settings","type":"input","defaultValue":"","hidden":false,"$$hashKey":"object:13141"}}},"apps":[],"jobName":"paragraph_1512471404803_-606237233","id":"20171123-183113_1224723343","dateCreated":"2017-12-05T19:56:44+0900","status":"READY","errorMessage":"","progressUpdateIntervalMs":500,"$$hashKey":"object:12652"},{"title":"Total Runtime For Each Setting","text":"%sql\nfrom AVG_DURATION select setting,sum(avgdur),count(*) group by setting order by sum(avgdur)/count(*)\n","user":"user1","dateUpdated":"2017-12-06T15:49:55+0900